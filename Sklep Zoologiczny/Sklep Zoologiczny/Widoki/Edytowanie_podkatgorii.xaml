<dx:ThemedWindow
    x:Class="Sklep_Zoologiczny.Widoki.Edytowanie_podkatgorii"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    Title="Edytowanie_podkatgorii" Height="800" Width="1000"
    >

    <Grid>
        <ComboBox x:Name="ComboBox_wybierz_podkategorie" HorizontalAlignment="Left" Margin="257,61,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Wybierz podkategorie:" HorizontalAlignment="Left" Margin="75,76,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <Label Content="Nowa nazwa podkategorii:" HorizontalAlignment="Left" Margin="102,132,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox x:Name="Wpisz_nowa_nazwe_podkategorii" HorizontalAlignment="Left" Margin="317,143,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <dx:SimpleButton x:Name="Zapisz_edytowanie_nazwy_podkategorii" Content="Zapisz" HorizontalAlignment="Left" Height="37" Margin="566,87,0,0" VerticalAlignment="Top" Width="133" Glyph="{dx:DXImage SvgImages/XAF/Action_SaveScript.svg}" Click="Zapisz_edytowanie_nazwy_podkategorii_Click"/>

    </Grid>
</dx:ThemedWindow>
