<dx:ThemedWindow
    x:Class="Sklep_Zoologiczny.Widoki.Usuwanie_kategorii"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    Title="Usuwanie_kategorii" Height="800" Width="1000"
    >

    <Grid>
        <Label Content="Usuń kategorię:" HorizontalAlignment="Left" Margin="71,67,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <dx:SimpleButton x:Name="Zapisz_usuwanie_kategorii" Content="Zapisz" HorizontalAlignment="Left" Height="31" Margin="485,78,0,0" VerticalAlignment="Top" Width="118" Glyph="{dx:DXImage SvgImages/XAF/Action_SaveScript.svg}" Click="Zapisz_usuwanie_kategorii_Click"/>
        <ComboBox x:Name="ComboBox_Kategoria" HorizontalAlignment="Left" Margin="250,78,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</dx:ThemedWindow>
