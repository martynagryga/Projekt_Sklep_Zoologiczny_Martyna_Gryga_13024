<dx:ThemedWindow
    x:Class="Sklep_Zoologiczny.Widoki.Edytowanie_producenta"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    Title="Edytowanie_producenta" Height="800" Width="1000"
    >

    <Grid>
        <dx:SimpleButton x:Name="Zapisz_edytowanie_nazwy_producenta" Content="Zapisz" HorizontalAlignment="Left" Height="46" Margin="634,94,0,0" VerticalAlignment="Top" Width="142" Glyph="{dx:DXImage SvgImages/XAF/Action_SaveScript.svg}" Click="Zapisz_edytowanie_nazwy_producenta_Click"/>
        <ComboBox x:Name="ComboBox_wybierz_producenta" HorizontalAlignment="Left" Margin="234,68,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Wybierz producenta:" HorizontalAlignment="Left" Margin="52,68,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <Label Content="Nowa nazwa producenta:" HorizontalAlignment="Left" Margin="71,117,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox x:Name="Wpisz_nowa_nazwe_producenta" HorizontalAlignment="Left" Margin="294,128,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

    </Grid>
</dx:ThemedWindow>
