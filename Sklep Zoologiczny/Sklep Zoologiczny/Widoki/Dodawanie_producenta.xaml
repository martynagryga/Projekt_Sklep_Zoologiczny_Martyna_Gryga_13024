<dx:ThemedWindow
    x:Class="Sklep_Zoologiczny.Widoki.Dodawanie_producenta"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    Title="Dodawanie_producenta" Height="800" Width="1000"
    >

    <Grid>
        <Label Content="Nazwa producenta:" HorizontalAlignment="Left" Margin="45,49,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox x:Name="Wpisz_nazwe_producenta" HorizontalAlignment="Left" Margin="230,60,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <dx:SimpleButton x:Name="Zapisz_nazwe_producenta" Content="Zapisz" HorizontalAlignment="Left" Height="32" Margin="437,71,0,0" VerticalAlignment="Top" Width="101" Glyph="{dx:DXImage SvgImages/XAF/Action_SaveScript.svg}" Click="Zapisz_nazwe_producenta_Click"/>

    </Grid>
</dx:ThemedWindow>
