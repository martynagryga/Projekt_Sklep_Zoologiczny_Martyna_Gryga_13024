<dx:ThemedWindow
    x:Class="Sklep_Zoologiczny.Widoki.Edytowanie_kategorii"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    Title="Edytowanie_kategorii" Height="800" Width="1000"
    >

    <Grid>
        <dx:SimpleButton x:Name="Zapisz_edytowanie_nazwy_kategorii" Content="Zapisz" HorizontalAlignment="Left" Height="53" Margin="605,79,0,0" VerticalAlignment="Top" Width="158" Glyph="{dx:DXImage SvgImages/XAF/Action_SaveScript.svg}" Click="Zapisz_edytowanie_nazwy_kategorii_Click"/>
        <Label Content="Wybierz kategorię:" HorizontalAlignment="Left" Margin="33,79,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <Label Content="Nowa nazwa kategorii:" HorizontalAlignment="Left" Margin="68,132,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox x:Name="Wpisz_nowa_nazwe_kategorii" HorizontalAlignment="Left" Margin="276,143,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="ComboBox_wybierz_kategorie" HorizontalAlignment="Left" Margin="236,90,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</dx:ThemedWindow>
